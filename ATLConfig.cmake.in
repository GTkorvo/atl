# - Config file for the Atl package
# It defines the following variables
#  ATL_INCLUDE_DIRS - include directories for Atl
#  ATL_LIBRARIES    - libraries to link against


include("${CMAKE_CURRENT_LIST_DIR}/ATLConfigVersion.cmake")

include(FindPackageHandleStandardArgs)
set(ATL_CONFIG "${CMAKE_CURRENT_LIST_FILE}")
find_package_handle_standard_args(ATL CONFIG_MODE)

if(NOT DEFINED ATL_BINARY_DIR)
  include("${CMAKE_CURRENT_LIST_DIR}/ATLTargets.cmake")
endif()
set(ATL_LIBRARIES ATL::atl)
set(ATL_INCLUDE_DIRS $<TARGET_PROPERTY:ATL::atl,INTERFACE_INCLUDE_DIRECTORIES>)
